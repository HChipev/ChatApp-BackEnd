{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SqlConnection": "{From environment variable}}"
  },
  "Jwt": {
    "Issuer": "HChipev ChatApp",
    "Audience": "Public",
    "Key": "{From environment variable}",
    "RefreshTokenValidityInDays": 365,
    "TokenValidityInDays": 7
  },
  "RabbitMQ": {
    "HostName": "{From environment variable}",
    "UserName": "{From environment variable}",
    "Password": "{From environment variable}",
    "VirtualHost": "{From environment variable}"
  },
  "Google": {
    "DefaultPassword": "{From environment variable}"
  },
  "FrontEndURL": "{From environment variable}"
}
